{
  "version": 0.5,
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./api/index.js"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "crons": [
    {
      "path": "/cron",
      "schedule": "*/2 * * * *"
    }
  ]
}
