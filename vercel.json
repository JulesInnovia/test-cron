{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "crons": [
    {
      "path": "/cron1",
      "schedule": "*/2 * * * *"
    },
    {
      "path": "/cron2",
      "schedule": "*/5 * * * *"
    }
  ]
}
